pipeline {
    agent any
    tools {
        maven 'Maven 3.8.5'
        jdk 'jdk9'
    }

    stages {
        stage ('Compile Stage') {

            steps {
                    sh 'mvn clean compile'
                }
        }

        stage ('Testing Stage') {

            steps {
                    sh 'mvn test'
                }
        }

    }
}
