pipeline {
    agent any
    tools { 
        maven 'Maven1 3.8.5' 
        jdk 'jdk9'

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'maven1 3.8.5') {
                    sh 'mvn clean compile'
                }
            }
        }

        stage ('Testing Stage') {

            steps {
                withMaven(maven : 'maven1 3.8.5') {
                    sh 'mvn test'
                }
            }
        }



        }
    }
}
